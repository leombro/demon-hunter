<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:anim/linear_interpolator"
    android:shareInterpolator="true">
    <translate android:fromXDelta="0" android:toXDelta="10" android:duration="50"/>
    <translate android:fromXDelta="0" android:toXDelta="-20" android:duration="50" android:startOffset="50"/>
    <translate android:fromXDelta="0" android:toXDelta="20" android:duration="50" android:startOffset="100"/>
    <translate android:fromXDelta="0" android:toXDelta="-20" android:duration="50" android:startOffset="150"/>
    <translate android:fromXDelta="0" android:toXDelta="20" android:duration="50" android:startOffset="200"/>
    <translate android:fromXDelta="0" android:toXDelta="-20" android:duration="50" android:startOffset="250"/>
    <translate android:fromXDelta="0" android:toXDelta="10" android:duration="50" android:startOffset="300"/>
    <translate android:fromXDelta="0" android:toXDelta="0" android:fromYDelta="0" android:toYDelta="10" android:duration="250" android:startOffset="350"/>
    <scale android:pivotY="50%" android:pivotX="50%" android:fromXScale="1.0" android:toXScale="1.2" android:fromYScale="1.0" android:toYScale="1.2"
        android:duration="250" android:startOffset="350"/>
    <translate android:fromXDelta="0" android:toXDelta="0" android:fromYDelta="0" android:toYDelta="-500" android:duration="200" android:startOffset="600"/>
    <scale android:pivotX="50%" android:pivotY="50%" android:fromXScale="1.0" android:toXScale="0.0" android:fromYScale="1.0" android:toYScale="0.0"
        android:duration="200" android:startOffset="600"/>
</set>